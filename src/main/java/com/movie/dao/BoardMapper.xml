<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boardMapper">
<!--   <select id="selectBoard" resultType="com.study.dto.BoardDTO"> -->
<!--     select * from table -->
<!--   </select> -->
<insert id="insertSeat" parameterType="String">
	insert into reservation_seat values(1,'guest',#{seat});
</insert>
<select id="selectRoom" parameterType="String" resultType="String">
	select theater_room_no from theater,theater_room where theater.no=theater_room.theater_no and theater.theater_name=#{theater} order by theater_room.theater_room_no asc;
</select>
</mapper>

